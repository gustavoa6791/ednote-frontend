{
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ],
    "env": {
      "development": {
        "plugins": [
          "transform-class-properties",
          "react-hot-loader/babel",
          "babel-plugin-transform-object-assign",
          [
            "@babel/plugin-transform-runtime",
            {
              "absoluteRuntime": false,
              "corejs": false,
              "helpers": true,
              "regenerator": true,
              "useESModules": false,
              "version": "7.0.0-beta.0"
            }
          ]
        ]
      }
    }
  }


    
